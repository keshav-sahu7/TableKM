/* Progress Bar */

QProgressBar
{
    background-color: rgb(36,36,36);
    text-align: center right;
    border : 1px solid rgb(36,36,36);
}

QProgressBar::chunk
{
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0.463277 rgb( 31, 163, 70), stop:1 rgb( 24, 124, 54))
}

QProgressBar:disabled
{
    background-color: rgb(72,72,72);
    border-color: transparent;				/* to maintain the size*/
}

QProgressBar::chunk:disabled
{
    background-color: rgb(110,110,110);
}


/* Slider */

QSlider::horizontal
{
    min-height: 18px;
}

QSlider:vertical
{
    min-width: 18px;
}

QSlider::groove:horizontal
{
    border: none;               /* If we remove this line, slider will look as we want */
    height: 2px;                /* The groove expands to the size of the slider by default.
                                    By giving it a height, it has a fixed size */
}

QSlider::groove:vertical
{
    border: none;              /* If we remove this line, slider will look as we want */
    width: 2px;                /* The groove expands to the size of the slider by default.
                                    By giving it a width, it has a fixed size */
}

QSlider::handle:horizontal
{
    image: url(:/icons/green/slider_handle.png);
    width: 16px;
    height: 16px;
    margin: -10px 0;        /* handle is placed by default on the contents rect of the groove.
                                Expand outside the groove */
}

QSlider::handle:vertical
{
    image: url(:/icons/green/slider_handle.png);
    width: 16px;
    height: 16px;
    margin: 0px -10px;      /* handle is placed by default on the contents rect of the groove.
                                Expand outside the groove */
}

QSlider::handle:disabled
{
    image: url(:/icons/green/slider_handle_disabled.png);
}

QSlider::add-page
{
    border: 1px solid rgb(36,36,36);
    background: rgb(36,36,36);
}

QSlider::add-page:disabled
{
    border-color: rgb(72,72,72);
    background: rgb(72,72,72);
}

QSlider::sub-page
{
    border: 1px solid rgb( 31, 163, 70);
    background: rgb( 31, 163, 70);
}

QSlider::handle:hover, QSlider::handle:focus
{
    image: url(:/icons/green/slider_handle_hover.png);
}

QSlider::sub-page:focus
{
    border: 1px solid rgb( 24, 124, 54);
    background: rgb( 24, 124, 54);
}

QSlider::sub-page:disabled
{
    border-color: rgb(110,110,110);
    background: rgb(110,110,110);
}


/* Group Box */

QGroupBox
{
    border: 1px solid rgb( 31, 163, 70);
    border-radius: 5px;
    margin-top: 4ex;
    padding-top: 2ex;
}

QGroupBox:disabled
{
    border-color: rgb(110,110,110);
}

QGroupBox::title
{
    subcontrol-origin: margin;
    subcontrol-position: top left; /* position at the top left */
    left : 10px;
    padding : 1px 2px;
    margin-top: 1px;			/* to show the border*/
    border: 1px solid rgb( 31, 163, 70);
    background-color: rgb( 31, 163, 70);
    border-radius: 5px;
}

QGroupBox::title:disabled
{
    background-color: rgb(110,110,110);
    border-color: rgb(110,110,110);
}

QGroupBox::indicator
{
    width: 16px;
    height: 16px;
}

QGroupBox::indicator:checked
{
    image: url(:/icons/green/checkbox_checked_with_bg.png)
}

QGroupBox::indicator:checked:hover
{
    image: url(:/icons/green/checkbox_checked_with_bg_hover.png)
}

QGroupBox::indicator:checked:disabled
{
    image: url(:/icons/green/checkbox_checked_with_bg_disabled.png)
}

QGroupBox::indicator:unchecked
{
    image: url(:/icons/green/checkbox_unchecked_with_bg.png)
}

QGroupBox::indicator:unchecked:hover
{
    image: url(:/icons/green/checkbox_unchecked_with_bg_hover.png)
}

QGroupBox::indicator:unchecked:disabled
{
    image: url(:/icons/green/checkbox_unchecked_with_bg_disabled.png)
}


/* Tool Box */

QToolBox::tab
{
    background-color: rgb( 31, 163, 70);
    padding-left: 4ex;
    border-top-left-radius: 15%;
    border-bottom-right-radius: 15%;
}

QToolBox::tab:selected
{
    background-color: rgb( 24, 124, 54);
    font: italic;
}

QToolBox::tab:disabled
{
    background-color: rgb(72,72,72);
}

QToolBox::tab:selected:disabled
{
    background-color: rgb(110,110,110);
}


/* Main Window Things*/

QStatusBar
{
    background-color: rgb(72,72,72);
}

QSizeGrip
{
    image: url(:/icons/green/size_grip.png);
    width: 16px;
    height: 16px;
}

QMainWindow::separator
{
    background-color: rgb( 72, 72, 72);
    width: 2px; /* when vertical */
    height: 2px; /* when horizontal */
}

QMainWindow::separator:hover
{
    background-color: rgb( 24, 124, 54);
}

QMainWindow::separator:disabled
{
    background-color: rgb(110,110,110);
}

QToolTip
{
    background-color: rgb( 20, 20, 20);
    color: rgb( 245, 245, 245);
    border: 1px solid rgb( 31, 163, 70);
}

/* dock widget */

QDockWidget
{
    titlebar-close-icon: url(:/icons/green/close.png);
    titlebar-normal-icon: url(:/icons/green/dock_widget_lock.png);
}

QDockWidget:disabled
{
    titlebar-close-icon: url(:/icons/green/close_disabled.png);
    titlebar-normal-icon: url(:/icons/green/dock_widget_lock_disabled.png);
}

QDockWidget::title
{
    text-align: left; /* align the text to the left */
    background: rgb( 31, 163, 70);
    padding-left: 5px;
}

QDockWidget::title:disabled
{
    background: rgb(110,110,110);
}

QDockWidget::close-button, QDockWidget::float-button
{
    icon-size: 10px;
}

/*
    If below dockwidget code is not working then comment them for the default behaviour
*/
QDockWidget::close-button:hover:!pressed, QDockWidget::float-button:hover:!pressed
{
    icon-size: 12px;
    background-color: rgb( 24, 124, 54);
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed
{
    icon-size: 10px;
    background-color: rgb( 24, 124, 54);
}


