/* Scroll Bar */

QScrollBar
{
    border: 2px solid rgb( 31, 163, 70);
    background-color: rgb(36, 36, 36);
}

QScrollBar:horizontal
{
    height: 15px;
    margin: 0px 20px 0px 20px;
}

QScrollBar::handle:horizontal
{
    background: rgb( 24, 124, 54);
    border-left: 2px solid rgb( 31, 163, 70);
    border-right: 2px solid rgb( 31, 163, 70);
    min-width: 20px;
    padding: 3px;
}

QScrollBar::add-line:horizontal
{
    border: 2px solid rgb( 31, 163, 70);	          /* required to make icons smaller*/
    background: rgb( 31, 163, 70);
    image : url(:/icons/green/pstv_sign.png);
    width: 20px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
    border: 2px solid rgb( 31, 163, 70);	          /* required to make icons smaller*/
    background: rgb( 31, 163, 70);
    image: url(:/icons/green/ngtv_sign.png);
    width: 20px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar:vertical
{
    width: 15px;
    margin: 20px 0px 20px 0px;
}

QScrollBar::handle:vertical
{
    background: rgb( 24, 124, 54);
    border-top: 2px solid rgb( 31, 163, 70);
    border-bottom: 2px solid rgb( 31, 163, 70);
    min-height: 20px;
    padding: 3px
}

QScrollBar::add-line:vertical
{
    border: 2px solid rgb( 31, 163, 70);	          /* required to make icons smaller*/
    background: rgb( 31, 163, 70);
    image : url(:/icons/green/pstv_sign.png);
    height: 20px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical
{
    border: 2px solid rgb( 31, 163, 70);	          /* required to make icons smaller*/
    background: rgb( 31, 163, 70);
    image: url(:/icons/green/ngtv_sign.png);
    height: 20px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar:handle:pressed, QScrollBar:add-line:pressed, QScrollBar:sub-line:pressed
{
    background-color : rgb( 24, 124, 54);
}

QScrollBar::disabled
{
    background-color: rgb( 72, 72, 72);
    border-color: rgb( 121, 121, 121);
}

QScrollBar::handle:disabled
{
    background-color: rgb(110,110,110);
    border-color: rgb(121, 121, 121);
}

QScrollBar::add-line:disabled
{
    background-color: rgb( 121, 121, 121);
    border-color: rgb( 121, 121, 121);
}

QScrollBar::sub-line:disabled
{
    background-color: rgb( 121, 121, 121);
    border-color: rgb( 121, 121, 121);
}




